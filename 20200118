const http = requir('http');
http.createServer(requestListner: functiion(request: response)){
//send HTTP
//HTTP 200 OK
response.writeHead(statusCode 200, header:{'Contest-Type': ''test/plain})
//send "hello world"
response.end(chunk: 'Hello Server')
}).listen(port:8888)

console.log('Server runing at http://127.0.0.1:8888/')
}

